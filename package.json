{
  "name": "BeBride-Server",
  "version": "1.0.0",
  "description": "Servi√ßo da parte Administrativa da BeBride",
  "main": "server.js",
  "author": "Miguel Duque Filho",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development node -r dotenv/config src/server.js",
    "dev": "NODE_ENV=development nodemon -r dotenv/config src/server.js --ignore __tests__",
    "production": "NODE_ENV=production node src/server.js",
    "migrate": "NODE_ENV=development  knex migrate:latest",
    "unmigrate": "NODE_ENV=development knex migrate:rollback",
    "seed": "NODE_ENV=development knex seed:run",
    "test": "NODE_ENV=test jest",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "consign": "^0.1.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.20.7",
    "mysql2": "^2.1.0"
  },
  "devDependencies": {
    "dotenv": "^8.2.0"
  }
}
